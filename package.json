{
  "name": "pickle",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:dev": "yarn prisma migrate dev",
    "db:migrate": "env-cmd -f .env.production prisma migrate deploy",
    "db:push": "yarn prisma db push",
    "db:seed": "yarn prisma db seed",
    "dev": "concurrently 'next dev' 'quirrel'",
    "graphcms": "env-cmd -f .env.local graphql-codegen",
    "lint": "next lint",
    "prepare": "husky install",
    "prisma": "env-cmd -f .env.local prisma",
    "start": "next start"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.4",
    "@nivo/core": "^0.73.0",
    "@nivo/line": "^0.73.0",
    "@prisma/client": "3.1.1",
    "@supabase/supabase-js": "^1.24.0",
    "@tailwindcss/typography": "^0.4.1",
    "axios": "^0.22.0",
    "boring-avatars": "^1.5.8",
    "date-fns": "^2.24.0",
    "graphql": "^15.6.0",
    "graphql-request": "^3.5.0",
    "ioredis": "^4.27.10",
    "lodash": "^4.17.21",
    "millify": "^4.0.0",
    "nanoid": "^3.1.28",
    "next": "11.1.2",
    "next-connect": "^0.10.2",
    "nookies": "^2.5.2",
    "pluralize": "^8.0.0",
    "quirrel": "^1.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^7.0.1",
    "sass": "^1.42.1",
    "swr": "^1.0.1",
    "tailwind-merge": "^0.7.0",
    "zod": "^3.9.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@types/chance": "^1.1.3",
    "@types/ioredis": "^4.27.6",
    "@types/lodash": "^4.14.175",
    "@types/pluralize": "^0.0.29",
    "@types/react": "17.0.26",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "autoprefixer": "^10.3.6",
    "babel-plugin-superjson-next": "^0.4.1",
    "chance": "^1.1.8",
    "concurrently": "^6.3.0",
